{
  "name": "s-sortbydistance",
  "version": "0.1.0",
  "description": "Sort array of strings by Levenshtein (Sift4) distance",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "mocha --require blanket --reporter mocha-lcov-reporter > etc/lcov.info && mocha --require blanket -R html-cov > etc/coverage.html",
    "lint": "jscs index.js --config=./etc/.jscs.json && jshint --reporter=node_modules/jshint-stylish index.js --config ./etc/.jshint.json",
    "test": "mocha",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastiansandqvist/s-sortbydistance.git"
  },
  "keywords": [
    "string",
    "distance",
    "sort",
    "levenshtein",
    "sift",
    "sift3",
    "sift4"
  ],
  "author": "sebastian sandqvist",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebastiansandqvist/s-sortbydistance/issues"
  },
  "homepage": "https://github.com/sebastiansandqvist/s-sortbydistance",
  "devDependencies": {
    "blanket": "1.1.6",
    "chai": "^3.2.0",
    "codeclimate-test-reporter": "^0.1.0",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2"
  },
  "dependencies": {
    "sift-distance": "^4.0.0"
  }
}
